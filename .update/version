#!/bin/bash
# shellcheck disable=SC2034
# Available DietPi version
G_REMOTE_VERSION_CORE=9
G_REMOTE_VERSION_SUB=19
G_REMOTE_VERSION_RC=-1
# Minimum DietPi version to allow update
G_MIN_VERSION_CORE=7
G_MIN_VERSION_SUB=0
# Alternative Git branch to automatically migrate to when DietPi version is too low
G_OLD_VERSION_BRANCH='8'
# Minimum Debian version to allow update
G_MIN_DEBIAN=6
# Alternative Git branch to automatically migrate to when Debian version is too low
G_OLD_DEBIAN_BRANCH='8'
# Live patches
G_LIVE_PATCH_DESC=('Fix DietPi-Dashboard installation')
G_LIVE_PATCH_COND=('grep -q '\''ravenclaw900/DietPi-Dashboard/workflows/push-build/rework-2'\'' /boot/dietpi/dietpi-software')
G_LIVE_PATCH=('sed --follow-symlinks -i '\''s|ravenclaw900/DietPi-Dashboard/workflows/push-build/rework-2|nonnorm/DietPi-Dashboard/workflows/push-build/main|'\'' /boot/dietpi/dietpi-software')
