			[[ -f '/etc/apt/trusted.gpg.d/dietpi-mympd.gpg' ]] && G_EXEC rm /etc/apt/trusted.gpg.d/dietpi-mympd.gpg
			[[ -d '/var/lib/mympd' ]] && G_EXEC rm -R /var/lib/mympd
